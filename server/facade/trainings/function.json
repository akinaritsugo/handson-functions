{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "queue",
      "direction": "out",
      "name": "waiting",
      "queueName": "waiting",
      "connection": "QUEUE_STORAGE_ACCOUNT"
    },
    {
      "type": "blob",
      "direction": "out",
      "name": "outImageBlob",
      "path": "images/{rand-guid}",
      "connection": "QUEUE_STORAGE_ACCOUNT"
    }
  ]
}
